<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.boss.springcloud.dao.OrderDao">

    <select id="queryById" parameterType="int" resultType="com.boss.springcloud.entity.Order">
      select * from order.order where typeid = #{typeid};
    </select>

    <select id="queryAll" resultType="com.boss.springcloud.entity.Order">
        select * from order.order;
    </select>

</mapper>